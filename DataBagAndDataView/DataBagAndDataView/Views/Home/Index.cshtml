
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        @ViewBag.MyCustomProperty
    </div>
    <div>
        <ul>
            @foreach (var item in ViewBag.MyList)
            {
                <li>@item</li>
            }
        </ul>
    </div>
    <div>
        <ul>
            @foreach (var item in ViewBag.MyEmpList)
            {
                <li>
                    Id - @item.Id, 
                    Name - @item.Name, 
                    Email - @item.Email
            </li>
            }
        </ul>
    </div>
</body>
</html>
